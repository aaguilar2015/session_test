option_settings:
  - namespace: aws:elasticbeanstalk:container:php:phpini
    option_name: document_root
    value: /public
  - namespace: aws:elasticbeanstalk:container:php:phpini
    option_name: max_execution_time
    value: 60
  - namespace: aws:elasticbeanstalk:container:php:phpini
    option_name: memory_limit
    value: 128M
  - namespace: aws:elasticbeanstalk:container:php:phpini
    option_name: allow_url_fopen
    value: false
